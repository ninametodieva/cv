{
  "extends": [
    "stylelint-config-standard",
    "stylelint-config-sass-guidelines",
    "stylelint-config-recess-order"
  ],
  "rules": {
    "max-nesting-depth": 3,
    "max-empty-lines": 2,
    "selector-max-compound-selectors": 5,
    "order/properties-alphabetical-order": null,
    "scss/at-rule-no-unknown": [ true, {
      "ignoreAtRules": [
        "util"
      ]
    }]
  }
}
